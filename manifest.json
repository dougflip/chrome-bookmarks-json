{
    "manifest_version": 2,
    "name": "Chrome Bookmarks JSON",
    "description": "Allows import/export of bookmarks at any level via JSON",
    "version": "1.0.0",
    "permissions": [
        "bookmarks",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["src/background.js"]
    }
}
